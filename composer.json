{
  "name": "philippoehrlein/kirby-navigation-groups",
  "description": "A Kirby Plugin for grouping navigation items",
  "type": "kirby-plugin",
  "license": "MIT",
  "version": "1.1.2",
  "homepage": "https://github.com/philippoehrlein/kirby-navigation-groups",
  "authors": [
      {
          "name": "Philipp Oehrlein",
          "email": "github@philippoehrlein.de"
      }
  ],
  "require": {
      "php": ">=8.0",
      "getkirby/composer-installer": "^1.2"
  },
  "require-dev": {
    "getkirby/cms": "^4.0 || ^5.0"
  },
  "autoload": {
    "psr-4": {
      "KirbyNavigationGroups\\": "lib/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "extra": {
      "kirby-cms-path": false
  }
}
